  "engines": {
    "node": "10.17.0",
    "npm": "6.11.3"
  },
  "scripts": {
    "postinstall": "cd node_modules/tamtam-frontend-builder && node utils/npm-install-recursive.js --dirs source/components,source/utilities --ignore node_modules",
    "bamboo": "tamtam-builder deploy",
    "deploy": "tamtam-builder deploy",
    "gulp": "tamtam-builder start",
    "start": "tamtam-builder start",
    "build": "tamtam-builder build",
    "dist": "tamtam-builder dist",
    "task": "tamtam-builder task",
    "create": "cd node_modules/tamtam-frontend-builder && node utils/create.js"
  },
  "devDependencies": {
    "tamtam-frontend-builder": "git+https://bitbucket.org/tamtam-nl/tamtam-frontend-builder.git#next"
